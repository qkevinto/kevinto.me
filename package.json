{
  "name": "kto",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "d": "rm -rf .development && hexo clean && hexo g && hexo deploy git",
    "c": "rm -rf .development && hexo clean",
    "g": "rm -rf .development && hexo generate",
    "s": "touch .development && hexo server",
    "test": "ava"
  },
  "hexo": {
    "version": "3.2.0"
  },
  "ava": {
    "files": [
      "**/*.test.js",
      "!**/public/**/*",
      "!**/node_modules/**/*",
      "!**/jspm_packages/**/*"
    ],
    "source": [
      "**/*.js",
      "!**/node_modules/**/*",
      "!**/jspm_packages/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "child-process-promise": "^2.0.1",
    "eslint": "^2.9.0",
    "hexo": "^3.2.0",
    "hexo-autoprefixer": "^1.0.0",
    "hexo-browsersync": "^0.2.0",
    "hexo-critical-css": "^1.0.5",
    "hexo-deployer-git": "^0.1.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-sass": "^0.2.0",
    "hexo-server": "^0.2.0",
    "jspm": "^0.16.33",
    "striptags": "^2.1.1",
    "stylelint": "^6.2.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "source"
    },
    "configFile": "source/jspm_config.js",
    "dependencies": {
      "lodash-es": "npm:lodash-es@^4.11.1",
      "striptags": "npm:striptags@^2.1.1",
      "text": "github:systemjs/plugin-text@^0.0.7",
      "twitter-fetcher": "npm:twitter-fetcher@^15.0.2",
      "whatwg-fetch": "npm:whatwg-fetch@^0.11.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      "npm:lodash-es@4.11.1": {
        "format": "esm"
      }
    }
  },
  "devDependencies": {
    "ava": "^0.14.0"
  }
}
